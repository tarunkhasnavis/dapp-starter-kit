#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/nx@18.0.6/node_modules/nx/bin/node_modules:/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/nx@18.0.6/node_modules/nx/node_modules:/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/nx@18.0.6/node_modules:/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/nx@18.0.6/node_modules/nx/bin/node_modules:/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/nx@18.0.6/node_modules/nx/node_modules:/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/nx@18.0.6/node_modules:/Users/tarunkhasnavis/Desktop/dapp-starter-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nx@18.0.6/node_modules/nx/bin/nx-cloud.js" "$@"
else
  exec node  "$basedir/../../../../../../nx@18.0.6/node_modules/nx/bin/nx-cloud.js" "$@"
fi
